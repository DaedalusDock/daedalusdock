{
  "private": true,
  "name": "tgui-workspace",
  "version": "5.0.1",
  "packageManager": "yarn@4.9.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "tgui:build": "webpack",
    "tgui:analyze": "webpack --analyze",
    "tgui:dev": "node --experimental-modules packages/tgui-dev-server/index.js",
    "tgui:lint": "eslint packages --ext .js,.cjs,.ts,.tsx",
    "tgui:prettier": "prettier --check .",
    "tgui:sonar": "eslint packages --ext .js,.cjs,.ts,.tsx -c .eslintrc-sonar.yml",
    "tgui:tsc": "tsc",
    "tgui:test": "jest --watch",
    "tgui:test-simple": "CI=true jest --color",
    "tgui:test-ci": "CI=true jest --color --collect-coverage",
    "tgui:prettier-fix": "prettier --write .",
    "tgui:eslint-fix": "eslint --fix packages --ext .js,.jsx,.cjs,.ts,.tsx"
  },
  "dependencies": {
    "@swc/core": "^1.11.29",
    "@swc/jest": "^0.2.38",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.29",
    "@types/webpack": "^5.28.5",
    "@types/webpack-env": "^1.18.8",
    "@typescript-eslint/parser": "^7.12.0",
    "common": "workspace:*",
    "css-loader": "^7.1.2",
    "esbuild-loader": "^4.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "file-loader": "^6.2.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.1.0",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.5.3",
    "sass": "^1.89.1",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.6",
    "typescript": "^5.8.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.99.9",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1"
  }
}
